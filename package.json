{
  "name": "@jporter-dev/nestjs-opensearch",
  "version": "1.0.0",
  "description": "Nest - modern, fast, powerful node.js web framework (@opensearch-project/opensearch)",
  "author": "Josh Porter",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "format": "prettier \"lib/**/*.ts\" --write",
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "publish:npm": "npm publish --access public",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0 || ^7.0.0 || ^8.0.0 || ^9.0.0 || ^11.0.0",
    "@opensearch-project/opensearch": "^1.1.0 || ^2.0.0 || ^3.0.0",
    "rxjs": "^6.2.1 || ^7.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-angular": "^20.0.0",
    "@nestjs/common": "11.1.10",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "13.1.3",
    "@types/node": "24.10.4",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.32.0",
    "husky": "^9.0.0",
    "lint-staged": "16.2.7",
    "prettier": "^3.0.0",
    "release-it": "^19.0.0",
    "rimraf": "6.1.2",
    "rxjs": "^7.8.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
